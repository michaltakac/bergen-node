{
  "name": "bergen",
  "version": "1.0.0",
  "description": "Generator for tests and series from TeX sources.",
  "main": "main.js",
  "bin": "./main.js",
  "scripts": {
    "gen": "node main.js",
    "build": "rm bergen && rm brgn && rm bergen.exe && pkg . --options max_old_space_size=4096",
    "pkg": "pkg"
  },
  "author": "Michal Takac <takacmichal92@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "fs-extra": "^8.1.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "minimist": "^1.2.0",
    "morgan": "^1.9.1",
    "node-latex": "^2.6.3"
  },
  "pkg": {
    "assets": [
      "exams/**/*",
      "exams/**/**/*",
      "exams/**/**/**/*",
      "series/**/*",
      "series/**/**/*",
      "series/**/**/**/*",
      "sources/**/*"
    ],
    "scripts": [
      "**.js"
    ]
  }
}
